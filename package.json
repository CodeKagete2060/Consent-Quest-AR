{
  "name": "consent-quest-monorepo",
  "version": "1.0.0",
  "description": "Consent Quest: Safety Sentinel - Monorepo for AI-powered digital safety education",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start:frontend": "cd frontend && npm run preview",
    "start:backend": "cd backend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "seed": "cd backend && npm run seed",
    "clean": "rm -rf frontend/node_modules backend/node_modules node_modules"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["safety", "education", "AI", "AR", "monorepo"],
  "author": "Consent Quest Team",
  "license": "MIT"
}